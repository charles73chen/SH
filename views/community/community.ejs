<body>
<div class='dashboard'>
    <div class="dashboard-nav">

        <nav class="dashboard-nav-list" id="menu">

        </nav>
    </div>
    <div class='dashboard-app'>

        <div class='dashboard-content'>
            <div class='container' id="result">
                <div class="card" style="width: 18rem;">
                    <img src="/images/1.jpg" class="card-img-top" alt="...">
                    <div class="card-body">
                        <h5 class="card-title">居樂</h5>
                        <p class="card-text"></p>
                        <a href="#" class="btn btn-primary">Go </a>
                    </div>
                </div>

            </div>
        </div>
    </div>
</div>

<script src="https://code.jquery.com/jquery-3.7.1.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.min.js" integrity="sha384-0pUGZvbkm6XF6gxjEnlmuGrJXVbNuzT9qBBavbLwCsOGabYfZo0T0to5eqruptLy"
        crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/gasparesganga-jquery-loading-overlay@2.1.1/dist/loadingoverlay.min.js"></script>
<script src="/javascripts/publicVar.js"></script>
<script src="/javascripts/publicFn.js"></script>
<script type="text/javascript" src="/javascripts/jquery.tablesorter.min.js"></script>
<script type="text/javascript" src="/javascripts/jquery.tablesorter.widgets.min.js"></script>
<script>
    const mobileScreen = window.matchMedia("(max-width: 990px )");
    $(document).ready(function () {
        getMenu();
        getCommunity();
    });

    function getCommunity(){
        //$.LoadingOverlay("show");
        ajaxdata.url = '/community/Ajax/GetAllCommunity';
        ajaxdata.success = function (msg) {
            //$.LoadingOverlay("hide");
            if (msg.state == '0') {
                var html = "";
                for (i = 0; i < msg.result.length; i++) {
                    html = html+"<div class='card' style='width: 18rem;'>"
                    html = html+"<img src='/images/1.jpg' class='card-img-top' alt='...'>"
                    html = html+"<div class='card-body'>"
                    html = html+"<h5 class='card-title'>"+msg.result[i].Community_NAME+"</h5>"
                    html = html+"<p class='card-text'></p>"
                    html = html+"<a href='#' class='btn btn-primary'>Go </a>"
                    html = html+"</div>"
                    html = html+"</div>";

                }
                //console.log(html)
                $('#result').html(html)

            } else {
                console.log(msg)
                alert(msg.msg);
            }


        };
        ajaxdata.data = {}
        $.ajax(ajaxdata);
    }


</script>
</body>
</html>
